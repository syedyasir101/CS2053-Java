class Lab {
}

class A {
    A() {
        System.out.println("I am in class A");
    }
}

class B extends A {
    B() {
        System.out.println("I am in class B");
    }
}

class C extends B {
    C() {
        System.out.println("I am in class C");
    }
}

public class Main {
    public static void main(String[] args) {
        C obj = new C();  
    }
}
